using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace IF_ELSE
{
    internal class Program
    {
        static void Main(string[] args)
        {   //IF-ELSE

            int x = Convert.ToInt32(Console.ReadLine());
            int y = 20;
            if (x < y)
            {
                Console.WriteLine(x + y);           
            }
            else
            {
                Console.WriteLine("cannot determine");
            }

            //IF-ELSEIF

            if (x == y)
            {
                Console.WriteLine(x += 1);
            }
            else if (x >= y)
            {
                Console.WriteLine(y *= 5);
            }
            else if((x<y && x <= y))
            {
                Console.WriteLine(x + y);
            }
            else
            {
                Console.WriteLine("cannot determine");
            }


            //SWITCH STATEMENT

            int s = 4;
            int result = 0;
            switch (s)
            {
                case 1:
                    result = 10;
                    break;

                case 2:
                    result = 5;
                    break;
                case 3:
                    result = 6;
                    break;
                default:
                    result = 200;
                    break;
                    
            }
            Console.WriteLine(result);
        }
    }
}
