using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Arithemtic_operators
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int x = 10, y = 20;
            int a = x + y;
            int b = x - y;
            int c = a * b;
            double d = c / b;
            Console.WriteLine(a);
            Console.WriteLine(b);
            Console.WriteLine(c);
            Console.WriteLine(d);
        }
    }
}
